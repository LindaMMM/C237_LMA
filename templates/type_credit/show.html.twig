            </tr>
            <tr>
                <th>Enable</th>
                <td>{{ type_credit.Enable ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_type_credit_index') }}">back to list</a>

    <a href="{{ path('app_type_credit_edit', {'id': type_credit.id}) }}">edit</a>

    {{ include('type_credit/_delete_form.html.twig') }}
