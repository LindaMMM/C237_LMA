{% extends 'base.html.twig' %}

{% block title %}TypeCredit{% endblock %}

{% block body %}
    <h1>TypeCredit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ type_credit.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ type_credit.Name }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ type_credit.Prix }}</td>
            </tr>
            <tr>
                <th>NbCredit</th>
